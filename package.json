  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tresero/directus-extension-audio-metadata-trigger.git"
  },
  "dependencies": {
    "music-metadata": "^7.13.0"
  },
  "directus:extension": {
    "type": "hook",
    "path": "src/index.js",
    "source": "src/index.js",
    "host": "^10.0.0"
  }
}